@model EosTestApp.Models.Organization
@{
    ViewBag.Title = "Добавление организации";
}
<form asp-action="CreateOrganization" asp-controller="home">
    <div class="form-group">
        <label asp-for="FullName" class="control-label">Полное наименование</label>
        <input type="text" asp-for="FullName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ShortName" class="control-label">Краткое наименование</label>
        <input type="text" asp-for="ShortName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ActivitySphereId" class="control-label">ID Сферы деятельности</label>
        <input type="text" asp-for="ActivitySphereId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DirectorFIO" class="control-label">ФИО директора</label>
        <input type="text" asp-for="DirectorFIO" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="AuthorizedCapital" class="control-label">Уставной капитал</label>
        <input type="text" asp-for="AuthorizedCapital" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="INN" class="control-label">ИНН</label>
        <input type="text" asp-for="INN" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="KPP" class="control-label">КПП</label>
        <input type="text" asp-for="KPP" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="OGRN" class="control-label">ОГРН</label>
        <input type="text" asp-for="OGRN" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>